{
  "name": "tree-sitter-mq",
  "version": "0.1.4",
  "description": "mq grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer",
    "mq"
  ],
  "author": "harehare",
  "license": "MIT",
  "dependencies": {
    "node-gyp-build": "^4.8.4",
    "node-addon-api": "^8.0.0"
  },
